CREATE TABLE `club` (
  `id` varchar(20) COLLATE utf8mb4_bin NOT NULL,
  `koreanName` varchar(45) COLLATE utf8mb4_bin DEFAULT NULL,
  `englishName` varchar(45) COLLATE utf8mb4_bin DEFAULT NULL,
  `gender` varchar(45) COLLATE utf8mb4_bin DEFAULT NULL,
  `personalNumber` varchar(45) COLLATE utf8mb4_bin DEFAULT NULL,
  `mobile` varchar(45) COLLATE utf8mb4_bin DEFAULT NULL,
  `phoneNumber` varchar(45) COLLATE utf8mb4_bin DEFAULT NULL,
  `emailAddress` varchar(45) COLLATE utf8mb4_bin DEFAULT NULL,
  `postNumber` varchar(45) COLLATE utf8mb4_bin DEFAULT NULL,
  `detailAddress` varchar(45) COLLATE utf8mb4_bin DEFAULT NULL,
  `membertype` varchar(45) COLLATE utf8mb4_bin DEFAULT NULL,
  `bank` varchar(45) COLLATE utf8mb4_bin DEFAULT NULL,
  `bankAccout` varchar(45) COLLATE utf8mb4_bin DEFAULT NULL,
  `payday` varchar(45) COLLATE utf8mb4_bin DEFAULT NULL,
  `inviterName` varchar(45) COLLATE utf8mb4_bin DEFAULT NULL,
  `passwd` varchar(45) COLLATE utf8mb4_bin DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_bin;
